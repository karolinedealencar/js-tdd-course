{
  "name": "js-tdd-course",
  "version": "1.0.0",
  "description": "Lib from Willian Justen TDD Course",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 80,
    "lines": 80,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karolinedealencar/js-tdd-course.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Karoline Alencar <karolinedalencar@gmail.com> (https://karolinealencar.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karolinedealencar/js-tdd-course/issues"
  },
  "homepage": "https://github.com/karolinedealencar/js-tdd-course#readme",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1"
  }
}
